{
  "name": "urql",
  "version": "0.0.3",
  "description": "A naive React Graphql library",
  "main": "lib/index.js",
  "scripts": {
    "clean-lib": "rimraf lib",
    "build-lib": "npm run clean-lib && BABEL_ENV=commonjs babel src -d lib --extensions \".ts,.tsx\"",
    "clean-es": "rimraf es",
    "build-es": "npm run clean-es && babel src -d es --extensions \".ts,.tsx\"",
    "watch-es": "watch 'npm run build-es' src/ -d",
    "build": "builder concurrent --buffer build-lib build-es",
    "demo:start:graphql-server": "node example/src/server/index.js",
    "demo:start:app": "webpack-dev-server --hot --inline --config 'example/webpack.config.js'",
    "type-check": "tsc",
    "lint": "eslint src",
    "test": "jest"
  },
  "author": "Ken Wheeler",
  "license": "MIT",
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.38",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.38",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.38",
    "@babel/polyfill": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-3": "^7.0.0-beta.38",
    "@babel/preset-typescript": "^7.0.0-beta.38",
    "@types/graphql": "^0.12.1",
    "@types/jest": "^22.0.1",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/uuid": "^3.4.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "builder": "^3.2.3",
    "cors": "^2.8.4",
    "eslint": "^4.15.0",
    "eslint-config-formidable": "^3.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql-tools": "^2.18.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.1.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0-0",
    "react-dom": "^16.0.0-0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "Microsoft/TypeScript#dontUseThisBranchForExperimentalPipelineInProductionOrYouWillBeFired",
    "typescript-eslint-parser": "^12.0.0",
    "uuid": "^3.2.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0-0",
    "react-dom": "^16.0.0-0"
  },
  "dependencies": {
    "create-react-context": "^0.1.3",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1"
  }
}
