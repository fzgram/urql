{
  "plugins": [
    "@babel/syntax-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-pipeline-operator",
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "root": ["."]
      }
    ]
  ],
  "presets": [
    "@babel/typescript",
    "@babel/react",
    "@babel/stage-3",
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "env": {
    "commonjs": {
      "presets": [
        "@babel/env",
        {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }
      ]
    }
  }
}
